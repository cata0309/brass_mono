#!/usr/bin/env bash
# Clean up generated files
set -e -o pipefail

bin/print start-first 'Removing FontForge files'
rm -v forge/*.sfd || true

bin/print start 'Removing generated fonts'
rm -v BrassMono/OpenType/*.otf || true
rm -v BrassMono/TrueType/*.ttf || true

rm -v BrassMono.zip || true
bin/print end 'Squeeky clean :D'
